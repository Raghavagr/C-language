// QUE) Write your own version of strcpy function from <string.h>

//URL: https://ide.geeksforgeeks.org/RZnxvTWDxV

//code
#include <stdio.h>
#include<string.h>

void copyStr(char *st, char *cst) {
    for(int i=0; st[i] != '\0'; i++) {
       cst[i] = st[i];  
    }
}

int main() {
	char st[30] = "Raghav";
	char cst[30];
	copyStr(st,cst);
	printf("%s",cst);
	return 0;
}
