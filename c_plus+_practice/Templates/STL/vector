#include <iostream>
#include<vector>

using namespace std;

void display(vector <int> &v1) {
    for(int i=0; i<v1.size(); i++) {
        cout<<v1[i]<<endl;
    }
}

int main() {
	vector <int> vec1;
	int size,element;
	for(int i=0;i<5;i++) {
	    cout<<"enter the element"<<endl;
	    cin>>element;
	    vec1.push_back(element);
	}
	//vec1.pop_back();   // we can also delete from back.
	display(vec1);
	
	return 0;
}
